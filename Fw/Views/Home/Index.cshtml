

<div class="row">
    <div class="col-md-4">
         <h2 id="title"></h2>
    </div>
    
</div>

<script>
    $(function () {
        fetch('/api/values', {
            method: 'GET',
            headers: {
                'Authorization': 'Bearer ' + localStorage.getItem('authToken')
            }
        }).then(response => response.json()).then(data => {
            $("#title").html("Welcome "+data.Firstname);
            console.log(data)
        });
    });
  
</script>